/////////////////////////
#define mx 300000


#define MAX(a, b) ((a) > (b) ? (a) : (b))
typedef complex<double> base;
double PI = acos(-1.);
void fft (vector<base> & a, bool invert) {
    int n = (int) a.size();

    for (int i=1, j=0; i<n; ++i) {
        int bit = n >> 1;
        for (; j>=bit; bit>>=1)
            j -= bit;
        j += bit;
        if (i < j)
            swap (a[i], a[j]);
    }

    for (int len=2; len<=n; len<<=1) {
        double ang = 2*PI/len * (invert ? -1 : 1);
        base wlen (cos(ang), sin(ang));
        for (int i=0; i<n; i+=len) {
            base w (1);
            for (int j=0; j<len/2; ++j) {
                base u = a[i+j],  v = a[i+j+len/2] * w;
                a[i+j] = u + v;
                a[i+j+len/2] = u - v;
                w *= wlen;
            }
        }
    }
    if (invert)
        for (int i=0; i<n; ++i)
            a[i] /= n;
}

void multiply (const vector<int> & a, const vector<int> & b, vector<int> & res) {
    vector<base> fa, fb;
    int i;

    int sz = a.size();
    for(i = 0; i < sz; i++)
        fa.push_back( a[i] );

    sz = b.size();
    for(i = 0; i < sz; i++)
        fb.push_back( b[i] );

    size_t n = 1;
    while (n < MAX (a.size(), b.size()))  n <<= 1;
    n <<= 1;
    fa.resize (n),  fb.resize (n);

    fft (fa, false),  fft (fb, false);
    for (i=0; i<n; ++i)
        fa[i] *= fb[i];
    fft (fa, true);

    res.resize (n);
    for (i=0; i<n; ++i)
        res[i] = int (fa[i].real() + 0.5);
}

int main() {
    int tc;
    I(tc);
    while(tc--) {
            
        vector<int>a(mx+1,0),b(mx+1,0),c;
        char a1[mx+2];
        SC("%s",a1+1);
        int l=strlen(a1+1);
        for(int i=l; i>=1; i--) {
            int x=a1[i]-'0';
            a[l-i+1]=x;
        }
        SC("%s",a1+1);
        int l1=strlen(a1+1);
        for(int i=l1; i>=1; i--) {
            int x=a1[i]-'0';
            b[l1-i+1]=x;
        }
        multiply(a,b,c);

        vector<int>ans;
        int co=0;
        for(int j=2; j<=l+l1; j++) {
            co+=c[j];
            ans.PB(co%10);
            co/=10;
        }
        if(co)ans.PB(co);
        bool fl=0;
        for(int i=-1+(int)ans.size(); i>=0; i--) {
            if(ans[i]==0&&fl==0)continue;
            fl=1;
            PI(ans[i]);
        }
        if(!fl)PI(0);
        NL;

    }


    return 0;
}
